<SerializableClass Type="OrderConfig" Version="1.0">
	<root>
		<common>
		</common>
		<orders>
			<order id="1">
				<type>PngHMConvert</type>
				<name>TestConvert</name>
				<params>
					<source>PngSrc</source>
					<destination>SVGMDst</destination>
				</params>
			</order>
			<order id="2">
				<type>Research</type>
				<name>GanttResearch</name>
				<params>
					<source>SVGMDst</source>
					<destination>RResDst</destination>
					<rewrite_dst>true</rewrite_dst>
				</params>
			</order>
		</orders>
		<data_standarts>
			<data_standart>
				<type>PngHeightMap</type>
				<name>PngSrc</name>
				<params>
					<file>..\..\..\scenarios\gantt_demo\gantt_demo.png</file>
				</params>
			</data_standart>
			<data_standart>
				<type>SurfaceViewerGenMap</type>
				<name>SVGMDst</name>
				<params>
					<folder>..\..\..\cache\gantt_demo\svgm</folder>
					<unit_data>..\..\..\scenarios\gantt_demo\gantt_demo.udp</unit_data>
					<env_stt>..\..\..\scenarios\gantt_demo\gantt_demo.es</env_stt>
					<!--map_stt>$(MAP_STT)</map_stt-->
					<pth_stt>..\..\..\scenarios\gantt_demo\gantt_demo.ps</pth_stt>
					<res_stt>..\..\..\scenarios\gantt_demo\gantt_demo.rs</res_stt>
					<!--sim_stt>$(SIM_STT)</sim_stt-->
				</params>
			</data_standart>
			<data_standart>
				<type>ResearchResult</type>
				<name>RResDst</name>
				<params>
					<folder>..\..\..\cache\gantt_demo\research</folder>
				</params>
			</data_standart>
		</data_standarts>
	</root>
</SerializableClass>